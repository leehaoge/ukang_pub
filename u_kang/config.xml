<?xml version='1.0' encoding='utf-8'?>
<widget id="com.lec.u_kang" version="0.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>U康</name>
    <description>
        U康健康平台
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Lanny Lee
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.3.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="cordova-sqlite-storage" spec="Cordova-sqlite-storage" />
    <plugin name="ukang-plugin-sleepace" spec="file:ukang-plugin-sleepace" />
    <plugin name="ukang-plugin-utils" spec="file:ukang-plugin-utils" />
</widget>
